    <div class="content">

      <div class="container text-left">
        <div class="row justify-content-center">
          <div class="col-lg-7">

            <h2 class="mb-5 text-center date">Add Checkin and Checkout Date</h2>
            <form action="" class="row" method="post">
              <div class="col-md-4">
                <div class="form-group">
                  <label for="input_from">From</label>
                  <input type="text" class="form-control" id="input_from" name="checkin" placeholder="Checkin" value="{{dates.checkin}}">
                </div>
              </div>
              <div class="col-md-4">
                <div class="form-group">
                  <label for="input_to">To</label>
                  <input type="text" class="form-control" id="input_to" name="checkout" placeholder="Checkout" value="{{dates.checkout}}">
                </div>
              </div>
              <div class="col-md-4">
                  <div class="form-group">
                    <label for="input_to">Guests</label>
                    <input type="number" class="form-control" name="guests" value="{{dates.guests}}" required>
                  </div>
                </div>
              <div class="col-md-6">
                <div class="form-group form-button">
                  <input type="submit" id="signup" class="form-submit" value="Save" />
                  <input type="hidden" name="packageid" value="{{package._id}}">
                </div>
              </div>
              <div class="messagefail">{{message}}</div>
            </form>

          </div>
        </div>

      </div>
    </div>
