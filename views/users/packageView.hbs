      <div class="content">

        <div class="container text-left">
          <div class="row justify-content-center">
            <div class="col-lg-7">

              <h2 class="mb-5 text-center date">Add Checkin and Checkout Date</h2>
              <form action="" class="row" method="post">
                <div class="col-md-4">
                  <div class="form-group">
                    <label for="input_from">From</label>
                    <input type="text" class="form-control" id="input_from" name="checkin" placeholder="Checkin" value="{{checkin}}" required>
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group">
                    <label for="input_to">To</label>
                    <input type="text" class="form-control" id="input_to" name="checkout" placeholder="Checkout" value="{{checkout}}" required>
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group">
                    <label for="input_to">Guests</label>
                    <input type="number" class="form-control" name="guests" placeholder="Number of guests" min="1" required>
                  </div>
                </div>
                <div class="col-md-6">
                  <div class="form-group form-button">
                    <input type="submit" id="signup" class="form-submit" value="Add To Bookings" />
                    <input type="hidden" name="packageid" value="{{package._id}}">
                  </div>
                </div>
                <div class="messagefail">{{message}}</div>
              </form>

            </div>
          </div>

        </div>
