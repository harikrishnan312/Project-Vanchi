    <br><br>

    {{!-- <section class="" style="background-color: ;"> --}}
      <div class="container py-5">
        <div class="row d-flex justify-content-center align-items-center">
          <div class="col-12">
            <div class="card card-registration card-registration-2" style="border-radius: 15px;">
              <div class="card-body p-0">
                <div class="row g-0">
                  <div class="col-lg-8">
                    <div class="p-5">
                      <div class="d-flex justify-content-between align-items-center mb-5">
                        <h1 class="fw-bold mb-0 text-black">Bookings</h1>
                      </div>
                      <div>
                        {{#if booking.length}}
                        {{#each booking}}

                        <hr class="my-1">
                        <div class="row mb-4 d-flex justify-content-between align-items-center">

                          <div class="col-md-8">
                            <h6 class="text-black "> Package : {{booking_id.0.package_name}}</h6>
                            {{#if booking_id.1.package_name}}
                            <h6 class="text-black "> Package :{{booking_id.1.package_name}}</h6>
                            {{/if}}
                            <h6 class="text-black "> Order ID : {{this.order_id}}</h6>
                            <br>
                            <h6 class="text-black "> Checkin : {{booking_id.0.checkin}}</h6>
                            <h6 class="text-black "> Checkout : {{booking_id.0.checkout}}</h6>
                            <br>
                            {{#if booking_id.1.checkin}}
                            <h6 class="text-black "> Checkin :{{booking_id.1.checkin}}</h6>
                            <h6 class="text-black "> Checkout :{{booking_id.1.checkout}}</h6>
                            {{/if}}
                            <h6 class="text-black "> Payment :{{payment_method}}</h6>
                            <h6 class="text-black "> Guests : {{booking_id.0.guests}}</h6>
                            {{#if booking_id.1.guests}}
                            <h6 class="text-black "> Guests :{{booking_id.1.guests}}</h6>
                            {{/if}}
                            <h6 class="text-black "> Booked Date :{{date}}</h6>
                            <br>
                            <h5 class="text-black mb-0">Total : {{this.total}}</h5>
                            <br>
                          </div>


                          <div class="col-md-3">
                            {{#if status}}
                            <button class="btn btn-success">Completed</button>
                            {{else}}
                            <button class="btn btn-warning">Not Completed</button>
                            <br>
                            <br>
                            <a href="/deleteBooking?id={{_id}}&dateid1={{booking_id.0.date_id}}&dateid2={{booking_id.1.date_id}}"
                              class="text-muted" data-toggle="modal" data-target="#exampleModalCenter{{@index}}"><button
                                class="btn btn-danger">Cancel Booking</button></a>
                            {{/if}}
                            <br><br>
                            {{#if is_blocked}}
                            <p class="text-danger">Booking is temporary blocked..</p>
                            {{/if}}
                          </div>
                        </div>
                        <!-- Modal -->
                        <div class="modal fade" id="exampleModalCenter{{@index}}" tabindex="-1" role="dialog"
                          aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
                          <div class="modal-dialog modal-dialog-centered" role="document">
                            <div class="modal-content">
                              <div class="modal-header">
                                <h5 class="modal-title" id="exampleModalLongTitle">Cancel</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                  <span aria-hidden="true">&times;</span>
                                </button>
                              </div>
                              <div class="modal-body">
                                Are you sure?
                              </div>
                              <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">No</button>
                                <a
                                  href="/deleteBooking?id={{_id}}&dateid1={{booking_id.0.date_id}}&dateid2={{booking_id.1.date_id}}"><button
                                    type="button" class="btn btn-primary">Yes</button></a>
                              </div>
                            </div>
                          </div>
                        </div>

                        {{/each}}
                        {{else}}
                        <div><img src="/images/cart empty.webp" alt="" class="img-fluid"></div>
                        {{/if}}
                      </div>


                    </div>
                  </div>

                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
